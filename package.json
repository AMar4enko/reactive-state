{
  "name": "reactive-state",
  "version": "3.0.0-alpha2",
  "description": "Redux-like state management using RxJS and TypeScript",
  "main": "src/index.js",
  "files": [
    "src/**/*.js",
    "src/**/*.js.map",
    "src/**/*.d.ts",
    "react/**/*.js",
    "react/**/*.js.map",
    "react/**/*.d.ts"
  ],
  "types": "src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "build-tests": "tsc -p test",
    "bundle": "webpack",
    "coverage": "node node_modules/.bin/istanbul cover _mocha -- test/test",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "prepublishOnly": "npm run build",
    "watch": "tsc -w --preserveWatchOutput",
    "watch-tests": "tsc -w -p test --preserveWatchOutput",
    "develop": "concurrently \"npm run watch\" \"npm run watch-tests\" ",
    "run-tests": "mocha --timeout 10000 test/test.js",
    "test": "npm run build-tests && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dynalon/reactive-state.git"
  },
  "keywords": [
    "Redux",
    "State",
    "reactive",
    "RxJS",
    "store",
    "React"
  ],
  "author": "Timo DÃ¶rr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dynalon/reactive-state/issues"
  },
  "homepage": "https://github.com/Dynalon/reactive-state",
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/enzyme": "^3.1.13",
    "@types/jsdom": "^11.0.4",
    "@types/mocha": "^2.2.41",
    "@types/node": "^9.6.7",
    "@types/node-fetch": "^1.6.7",
    "chai": "^3.5.0",
    "concurrently": "^3.4.0",
    "coveralls": "^3.0.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "istanbul": "^0.4.5",
    "jsdom": "^11.10.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "node-fetch": "^1.7.1",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@types/lodash.isobject": "^3.0.3",
    "@types/lodash.isplainobject": "^4.0.3",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "lodash.isobject": "^3.0.2",
    "lodash.isplainobject": "^4.0.6",
    "rxjs": "^6.2.2"
  },
  "optionalDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "redux": "^4.0.0"
  }
}
